{
  "name": "rivieratravel/opentelemetry-auto-automapper",
  "description": "OpenTelemetry auto-instrumentation for automapper",
  "keywords": ["opentelemetry", "otel", "open-telemetry", "tracing", "automapper", "instrumentation"],
  "type": "library",
  "homepage": "https://opentelemetry.io/docs/php",
  "readme": "./README.md",
  "license": "Apache-2.0",
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "ext-opentelemetry": "*",
    "open-telemetry/api": "^1.0",
    "open-telemetry/sem-conv": "^1.23",
    "mark-gerarts/auto-mapper-plus": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "phan/phan": "^5.0",
    "php-http/mock-client": "*",
    "phpstan/phpstan": "^1.1",
    "phpstan/phpstan-phpunit": "^1.0",
    "psalm/plugin-phpunit": "^0.16",
    "open-telemetry/sdk": "^1.0",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Riviera\\Contrib\\Instrumentation\\AutoMapper\\": "src/"
    },
    "files": [
      "_register.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Riviera\\Tests\\Instrumentation\\AutoMapper\\": "tests/"
    }
  }
}
